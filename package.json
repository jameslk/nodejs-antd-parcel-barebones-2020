{
  "name": "wps-portal",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "nodemon ./dist/server",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "watch:prettier": "onchange 'src/**/*.ts*' -- prettier --write {{changed}}",
    "watch:server": "nodemon ./dist/server",
    "watch": "npm-run-all --parallel watch:*"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1.2"
  }
}
